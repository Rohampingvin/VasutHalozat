<Window x:Class="VasuthalozatAdmin.view.NewCityPicker"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VasuthalozatAdmin.view"
        mc:Ignorable="d"
        Title="NewCityPicker" Style="{StaticResource ResourceKey=BaseWindowStyle}">
    <Grid>
        <Button Content="Hozzáadás" Style="{StaticResource ResourceKey=AddCity}"/>
        <Button Content="Mégse" Style="{StaticResource ResourceKey=AddCityBack}"/>
        <TextBox x:Name="AddCityInput" Style="{StaticResource ResourceKey=AddCityInput}"/>
    </Grid>
</Window>
